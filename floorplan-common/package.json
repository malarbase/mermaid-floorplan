    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "shx": "~0.4.0",
    "typescript": "~5.8.3",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "floorplan",
    "geometry",
    "utilities"
  ]
}

