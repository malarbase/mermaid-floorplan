{
  "name": "floorplan-common",
  "version": "0.1.0",
  "description": "Shared utilities for floorplan packages (geometry, types, constants)",
  "license": "MIT",
  "author": "Malar Kannan",
  "type": "module",
  "main": "./out/index.js",
  "types": "./out/index.d.ts",
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "import": "./out/index.js",
      "default": "./out/index.js"
    }
  },
  "files": [
    "out"
  ],
  "scripts": {
